generator client {
  provider = "prisma-client-js"
}

generator erd {
  provider = "prisma-erd-generator"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Bets {
  id       String   @id @default(cuid())
  title    String
  code     String   @unique
  createAt DateTime @default(now())
}
